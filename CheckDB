function verificarOQueOSistemaVe() {
  try {
    const abaProcessos = TVOCore.CONFIG.SHEET_NAMES.PROCESSOS;
    const ssId = TVOCore.CONFIG.IDS.SPREADSHEET;
    
    const ss = SpreadsheetApp.openById(ssId);
    const ws = ss.getSheetByName(abaProcessos);
    
    if (!ws) {
      Logger.log("âŒ ERRO: A aba '" + abaProcessos + "' nÃ£o existe na planilha!");
      return;
    }
    
    const valores = ws.getDataRange().getValues();
    Logger.log("âœ… Aba encontrada: " + abaProcessos);
    Logger.log("ğŸ“Š Total de linhas detectadas (incluindo cabeÃ§alho): " + valores.length);
    
    if (valores.length > 0) {
      Logger.log("ğŸ“ CabeÃ§alhos na planilha: " + valores[0].join(" | "));
    }
    
    const processos = TVOCore.Repo_Processos.listar();
    Logger.log("ğŸš€ Resultado do Repo_Processos.listar(): " + processos.length + " itens.");
    
  } catch (e) {
    Logger.log("âŒ ERRO CRÃTICO: " + e.message);
  }
}
